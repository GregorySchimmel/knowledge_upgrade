// 
// Special characters
//
// \n = new line
// \t = tab
// \\ = backslash
// \' = single quote
// \" = double quote
//
//gs.info('Single string\nTwo lines');
console.log('Single string\nTwo lines');

//gs.info('Greg\'s simple script');
console.log('Greg\'s simple script');

//gs.info('Don\'t confuse a forward slash (/) with a backslash(\\)');
console.log('Don\'t confuse a forward slash (/) with a backslash(\\)');
