//Need to work in user input and displaying team once and waterfall players after team.

var id = 1;

for (var team = 1; team < 6; team++) {
  for (var player = 0; player <= 6; player++) {
  player ++;
    console.info('team=' + team + ' player=' + player + ' id=' + id);
    id++;
  }
}
console.info('Done creating teams and player');
